%h1 Listing customer_feedbacks

%table
  %tr
    %th Subject
    %th Body
    %th Customer
    %th Viewed
    %th
    %th
    %th

  - @customer_feedbacks.each do |customer_feedback|
    %tr
      %td= customer_feedback.subject
      %td= customer_feedback.body
      %td= customer_feedback.customer_id
      %td= customer_feedback.viewed
      %td= link_to 'Show', customer_feedback
      %td= link_to 'Edit', edit_customer_feedback_path(customer_feedback)
      %td= link_to 'Destroy', customer_feedback, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Customer feedback', new_customer_feedback_path
